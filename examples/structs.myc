struct Point {
    x int
    y int
}

// structs within structs
struct Line {
    begin Point
    end   Point
}

var p1 = Point{1, 4}
var p2 = Point{x: 3, y: 4}
var p3 = Point{
    x: 56,
    y: 78,
}

print p1
print p2.x + p3.y
print p2.y + p3.x

var l = Line{
    begin: p2,
    end:   p3,
}

fun xsum(l Line) int {
    return l.begin.x + l.end.x
}

fun ysum(l Line) int {
    return l.begin.y + l.end.y
}

print l
print xsum(l)
print ysum(l)
